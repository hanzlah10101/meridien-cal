{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/**/*": {
      "memory": 256,
      "maxDuration": 10,
      "includeFiles": "{assets/**/*}"
    }
  },
  "buildCommand": "pnpm build",
  "outputDirectory": "",
  "redirects": [
    {
      "source": "/",
      "destination": "/assets/public/Hotels.html",
      "permanent": false
    }
  ]
}
